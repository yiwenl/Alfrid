  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.7",
    "dat-gui": "^0.5.0",
    "eslint": "^4.7.1",
